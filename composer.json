{
    "name": "blackends/laravel-generator",
    "description": "BlackEnds Laravel Generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "BlackEnds",
            "email": "blackends@gmail.com"
        },
        {
            "name": "Zubair Iftikhar",
            "email": "zu0072010@gmail.com",
            "homepage": "https://blackends.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "prettus/l5-repository": "^2.7.9",
        "laracasts/flash": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    }    
}
